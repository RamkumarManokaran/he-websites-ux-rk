{
  "name": "my-turborepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/whatuni",
    "apps/pgs"
  ],
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "turbo build && npm run build:cdk",
    "dev": "turbo dev",
    "lint": "eslint .",
    "test": "jest",
    "dev:pgs": "npm run dev --workspace=pgs",
    "build:pgs": "npm run build --workspace=pgs",
    "start:pgs": "npm run start --workspace=pgs",
    "open-next:pgs": "cd apps/pgs && open-next build",
    "cdk-synth:pgs": "cd apps/pgs/cdk && cdk synth --build",
    "cdk-diff:pgs": "cd apps/pgs/cdk && cdk diff",
    "cdk-deploy:pgs": "cd apps/pgs/cdk && cdk deploy -- --all --require-approval never --ci",
    "dev:whatuni": "npm run dev --workspace=whatuni",
    "build:whatuni": "npm run build --workspace=whatuni",
    "start:whatuni": "npm run start --workspace=whatuni",
    "open-next:whatuni": "cd apps/whatuni && open-next build",
    "cdk-synth:whatuni": "cd apps/whatuni/cdk && cdk synth --build",
    "cdk-diff:whatuni": "cd apps/whatuni/cdk && cdk diff",
    "cdk-deploy:whatuni": "cd apps/whatuni/cdk && cdk deploy -- --all --require-approval never --ci",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.10.0",
    "open-next": "^3.1.3",
    "prettier": "^3.2.5",
    "turbo": "^2.1.1",
    "typescript": "~5.5.3",
    "typescript-eslint": "^8.8.0",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "aws-cdk": "2.149.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@repo/typescript-config": "^0.0.0",
    "tailwindcss": "^3.4.10",
    "aws-cdk-lib": "2.149.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2"
}
