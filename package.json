{
  "name": "my-turborepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/whatuni",
    "apps/pgs"
  ],
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "turbo build && npm run build:cdk",
    "dev": "npm run dev --workspace=whatuni",
    "lint": "eslint .",
    "test": "jest",
    "dev:pgs": "npm run dev --workspace=pgs",
    "build:pgs": "npm run build --workspace=pgs",
    "start:pgs": "npm run start --workspace=pgs",
    "open-next:pgs": "cd apps/pgs && open-next build",
    "cdk-synth:pgs": "cd apps/pgs/cdk && npm install && cdk synth --build",
    "cdk-diff:pgs": "cd apps/pgs/cdk && cdk diff",
    "cdk-deploy:pgs": "cd apps/pgs/cdk && cdk deploy -- --all --require-approval never --ci",
    "dev:whatuni": "npm run dev --workspace=whatuni",
    "build:whatuni": "npm run build --workspace=whatuni",
    "start:whatuni": "npm run start --workspace=whatuni",
    "open-next:whatuni": "cd apps/whatuni && open-next build",
    "cdk-synth:whatuni": "cd apps/whatuni/cdk && npm install && cdk synth --build",
    "cdk-diff:whatuni": "cd apps/whatuni/cdk && cdk diff",
    "cdk-deploy:whatuni": "cd apps/whatuni/cdk && cdk deploy -- --all --require-approval never --ci",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.1",
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@eslint/js": "^9.12.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.17.0",
    "autoprefixer": "^10.4.20",
    "aws-cdk": "2.149.0",
    "babel-jest": "^29.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "open-next": "^3.1.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.1.1",
    "typescript-eslint": "^8.8.0"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "next": "14.2.14",
    "@babel/preset-react": "^7.25.7",
    "@next/third-parties": "^15.0.1",
    "aws-cdk-lib": "2.149.0",
    "constructs": "^10.0.0",
    "i": "^0.3.7",
    "npm": "^10.9.0",
    "react-device-detect": "^2.2.3",
    "react-facebook-pixel": "^1.0.4",
    "source-map-support": "^0.5.21",
    "swiper": "^11.1.14",
    "tailwindcss": "^3.4.10"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2"
}
